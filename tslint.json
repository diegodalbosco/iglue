{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ true, { "multiline": "never", "singleline": "never" } ],
    "object-literal-sort-keys": false,
    "space-before-function-paren": ["error", "always"],
    "forin": false
  },
  "rulesDirectory": []
}
